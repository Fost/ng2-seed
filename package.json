      "rxjs"
    ],
    "contributors": [
      "pi_men"
    ],
    "dependencies": {
        "@angular/common": "4.0.0-rc.1",
        "@angular/compiler": "4.0.0-rc.1",
        "@angular/core": "4.0.0-rc.1",
        "@angular/forms": "4.0.0-rc.1",
        "@angular/http": "4.0.0-rc.1",
        "@angular/platform-browser": "4.0.0-rc.1",
        "@angular/platform-browser-dynamic": "4.0.0-rc.1",
        "@angular/platform-server": "4.0.0-rc.1",
        "@angular/router": "4.0.0-rc.1",

        "@angular/material": "2.0.0-beta.2",

        "@ngrx/core": "1.2.0",
        "@ngrx/effects": "2.0.0",
        "@ngrx/store": "2.2.1",
        "@ngrx/store-devtools": "3.2.3",

        "rxjs": "5.2.0",

        "jquery": "3.1.1",

        "core-js": "2.4.1",
        "ie-shim": "0.1.0",

        "zone.js": "0.7.7",

        "immutable": "3.8.1",
        "ts-md5": "1.2.0"
    },
    "devDependencies": {
        "express": "4.14.1",

        "@types/jasmine": "2.5.43",
        "@types/node": "7.0.5",
        "@types/source-map": "0.5.0",
        "@types/uglify-js": "2.6.28",
        "@types/webpack": "2.2.7",

        "codelyzer": "2.0.1",
        "node-sass": "4.5.0",
        "autoprefixer": "6.7.5",
        "postcss": "5.2.15",
        "precss": "1.4.0",
        "postcss-smart-import": "0.6.9",
        "postcss-flexbugs-fixes": "2.1.0",
        "sass-lint": "1.10.2",
        "rimraf": "2.6.1",

        "es6-promise": "4.0.5",
        "es6-promise-loader": "1.0.2",
        "es6-shim": "0.35.3",
        "es7-reflect-metadata": "1.6.0",

        "copy-webpack-plugin": "4.0.1",
        "compression-webpack-plugin": "0.3.2",
        "extract-text-webpack-plugin": "2.0.0",
        "script-ext-html-webpack-plugin": "1.7.1",
        "html-webpack-plugin": "2.28.0",
        "add-asset-html-webpack-plugin": "1.0.2",

        "angular-router-loader": "0.5.0",
        "angular2-template-loader": "0.6.2",
        "awesome-typescript-loader": "3.0.7",
        "exports-loader": "0.6.4",
        "expose-loader": "0.7.3",
        "file-loader": "0.10.1",
        "imports-loader": "0.7.1",
        "json-loader": "0.5.4",
        "raw-loader": "0.5.1",
        "postcss-loader": "1.3.2",
        "css-loader": "0.26.2",
        "sass-loader": "6.0.2",
        "style-loader": "0.13.2",
        "source-map-loader": "0.1.6",
        "resolve-url-loader": "2.0.0",
        "string-replace-loader": "1.0.5",
        "url-loader": "0.5.8",
        "tslint-loader": "3.4.2",
        "to-string-loader": "1.1.5",

        "ts-helpers": "1.1.2",
        "ts-node": "2.1.0",
        "tslint": "4.4.2",
        "typedoc": "0.5.7",
        "typescript": "2.2.1",

        "webpack": "2.2.1",
        "webpack-dev-server": "2.4.1",
        "webpack-md5-hash": "0.0.5",
        "webpack-merge": "3.0.0",
        "webpack-dashboard": "0.3.0"
    },
    "scripts": {
        "test": "karma start",

        "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
        "clean:dist": "npm run rimraf -- dist",

        "lint": "tslint \"src/**/*.ts\"",

        "build": "npm run build:dev",
        "build:dev": "webpack --config config/webpack.dev.js",
        "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js",

        "build:dll": "webpack --config config/webpack.dll.js --progress",

        "server": "npm run server:dev",
        "server:dev": "webpack-dev-server --config config/webpack.dev.js",
        "server:dev:dash": "webpack-dashboard -- webpack-dev-server --config config/webpack.dev.js",
        "server:dev:hmr": "npm run server:dev -- --hot",

        "server:prod": "node server.js",

        "start": "npm run server",

        "docs": "typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
        "docs-pages": "wintersmith build -C docs",
        
        "heroku:run": "npm run build:dll && npm run build:prod && npm run server:prod",
        "heroku:deploy": "git push origin master && git push heroku master",

        "rimraf": "rimraf"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Fost/ng2-seed.git"
    },
    "bugs": {
        "url": "https://github.com/Fost/ng2-seed/issues"
    },
    "engines": {
        "node": "6.9.2",
        "npm": "3.10.9"
    }
}
